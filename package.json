{
	"name": "mellov-web-app",
	"sideEffects": [
		"*.css"
	],
	"version": "1.0.0",
	"description": "Mellov web application",
	"private": true,
	"scripts": {
		"copy-assets": "cpx \"public/**/*\" dist",
		"start": "npm run serve",
		"build": "webpack --config webpack.prod.js",
		"serve": "node server.js",
		"serve:dev": "webpack serve --config webpack.dev.js",
		"test": "jest --collectCoverage"
	},
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	},
	"babel": {
		"plugins": [
			[
				"babel-plugin-transform-builtin-extend",
				{
					"globals": [
						"Error",
						"Array"
					]
				}
			],
			[
				"@babel/plugin-proposal-class-properties",
				{
					"loose": true
				}
			],
			[
				"@babel/plugin-proposal-private-methods",
				{
					"loose": true
				}
			],
			[
				"@babel/plugin-proposal-private-property-in-object",
				{
					"loose": true
				}
			]
		],
		"env": {
			"test": {
				"plugins": [
					"@babel/plugin-transform-modules-commonjs"
				]
			}
		}
	},
	"author": "Konrad Cielecki",
	"license": "ISC",
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.11.2",
		"@types/jest": "^29.2.5",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"amazon-cognito-identity-js": "^6.1.1",
		"antd": "^4.4.3",
		"express": "^4.17.1",
		"mellov_api": "https://github.com/conrisc/mellov-api-client",
		"path": "^0.12.7",
		"querystring": "0.2.0",
		"react": "^16.10.2",
		"react-dom": "^16.10.2",
		"react-redux": "^7.1.3",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.5",
		"superagent": "^5.1.0",
		"throttle-debounce": "^2.1.0",
		"ts-loader": "^9.4.2",
		"typescript": "^4.9.4"
	},
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/plugin-proposal-class-properties": "^7.5.5",
		"@babel/plugin-transform-modules-commonjs": "^7.6.0",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.16.7",
		"@webpack-cli/serve": "^1.6.0",
		"babel": "^6.23.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-transform-builtin-extend": "^1.1.2",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-react-app": "^9.0.2",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.0.4",
		"cpx": "^1.5.0",
		"css-loader": "^3.2.0",
		"dotenv-webpack": "^7.0.3",
		"file-loader": "^4.2.0",
		"imports-loader": "^3.1.1",
		"jest": "^24.9.0",
		"less": "^3.13.0",
		"less-loader": "^7.1.0",
		"style-loader": "^1.0.0",
		"svg-inline-loader": "^0.8.0",
		"webpack": "^5.66.0",
		"webpack-cli": "^4.3",
		"webpack-dev-middleware": "^5.3.0",
		"webpack-dev-server": "^4.7.3",
		"webpack-hot-middleware": "^2.25.1",
		"webpack-merge": "^5.8.0"
	}
}
